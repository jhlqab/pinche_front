.mag-btn:after{ display: none; }
.mag-btn.button-hover{ opacity: .8; }
.mag-submit-btn{ height: 48px; line-height: 48px; }
.mag-agree-control{ color:#9b9b9b; }

.has-hairline{ position: relative; }
.hairline-in-pseudo-top:after{
    content:" "; position: absolute; left:0; top:0; width:100%; height:0; transform:scaleY(0.5); 
    -webkit-transform:scaleY(0.5); border-top:1px solid rgba(0,0,0,.1);
}
.hairline-in-pseudo-bottom:after{
    content:" "; position: absolute; left:0; bottom:0; width:100%; height:0; transform:scaleY(0.5); 
    -webkit-transform:scaleY(0.5); border-bottom:1px solid rgba(0,0,0,.1);
}
.hairline-in-pseudo-left:after{
    content:" "; position: absolute; left:0; top:0; width:0; height:100%; transform:scaleX(0.5); 
    -webkit-transform:scaleX(0.5); border-left:1px solid rgba(0,0,0,.1);
}
.hairline-in-pseudo-right:after{
    content:" "; position: absolute; left:0; right:0; width:0; height:100%; transform:scaleX(0.5); 
    -webkit-transform:scaleX(0.5); border-right:1px solid rgba(0,0,0,.1);
}
.hairline-in-pseudo-fouredges:after{
    content:" "; position: absolute; left:0; top:0; width:200%; height:200%; transform:scale(0.5); 
    -webkit-transform:scale(0.5); transform-origin: 0 0; -webkit-transform-origin: 0 0; border:1px solid rgba(0,0,0,.1);
}

.sex{ width:12px; height: 12px; background-position: left top; background-repeat: no-repeat; background-size: 12px 12px; }
.sex.male{ background-image: url(data:image/png;base64,); }
.sex.female{ background-image: url(data:image/png;base64,); }

