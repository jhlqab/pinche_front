.list-address .arr{ display: inline-block; width:17px; height:10px; margin:0 3px;
    background: url(data:image/png;base64,) no-repeat left top; background-size: 17px 10px; }
.list-time{ font-size: 14px; color:#666; margin-top: 6px; line-height: 20px; }
.list-time .day{ color:#FF0000; margin-right: 4px; }

.list-sub{ margin: 6px 0 0 0; }
.list-sub .note{ display: block; font-size: 14px; line-height: 20px; color:#666; }
